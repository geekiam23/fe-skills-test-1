{
  "name": "ndustrial-react-starter",
  "version": "0.1.0",
  "description": "ndustrial.io react starter application",
  "main": "app.js",
  "scripts": {
    "build": "npm run prod-build",
    "clean": "rimraf dist",
    "compile": "webpack",
    "coverage": "NODE_ENV=test nyc mocha -R spec 'src/**/*.spec.js'",
    "lint": "npm run lint:js; npm run lint:styles",
    "lint:js": "eslint '+(src|test)/**/*.js'",
    "lint:styles": "stylelint './src/**/*.scss';",
    "prod-start": "NODE_ENV=production node src/server.js",
    "prod": "NODE_ENV=production nodemon src/server.js",
    "prod-build": "NODE_ENV=production webpack --config webpack.production.config.js",
    "serve": "npm run prod-start",
    "report": "nyc report --reporter=lcov",
    "start": "webpack-dev-server --color",
    "test": "NODE_ENV=test mocha -R spec 'src/**/*.spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndustrialio/react-starter.git"
  },
  "keywords": [
    "react",
    "webpack",
    "nodejs",
    "node"
  ],
  "author": "ndustrial.io <dev@ndustrial.io> (ndustrial.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ndustrialio/react-starter/issues"
  },
  "homepage": "https://github.com/ndustrialio/react-starter#readme",
  "devDependencies": {
    "@ndustrial/stylelint-config-ndustrial": "latest",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "breakpoint-sass": "^2.7.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "enzyme": "^2.8.0",
    "eslint": "^4.14.0",
    "eslint-config-ndustrial": "^1.0.5",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "^2.30.1",
    "imagemin-webpack-plugin": "^1.5.2",
    "jsdom": "^11.5.1",
    "mocha": "^3.5.3",
    "mock-local-storage": "^1.0.5",
    "mz": "^2.7.0",
    "nock": "^9.2.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.7",
    "nyc": "^11.5.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^15.6.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "supertest": "^3.0.0",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-dev-server": "^2.9.7",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
