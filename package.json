    "body-parser": "^1.18.2",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "history": "^4.6.3",
    "lodash": "^4.17.2",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "recharts": "^1.0.0-beta.7",
    "sinon": "^2.3.4",
    "url-parse": "^1.2.0"
  }
